    "blueprint",
    "cli"
  ],
  "repository": "eush77/unist-builder-blueprint-cli",
  "bugs": "https://github.com/eush77/unist-builder-blueprint-cli/issues",
  "author": "Eugene Sharygin <eush77@gmail.com>",
  "contributors": [
    "Eugene Sharygin <eush77@gmail.com>",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "bin": {
    "unist-builder-blueprint": "cli.js"
  },
  "files": [
    "cli.js",
    "lib/"
  ],
  "dependencies": {
    "escodegen": "^1.0.0",
    "help-version": "^1.0.0",
    "mdast-util-heading-range": "^2.0.0",
    "mdast-util-to-string": "^1.0.0",
    "minimist": "^1.0.0",
    "or-die": "0.0.1",
    "read-file-stdin": "^0.2.0",
    "readme-file": "^0.2.0",
    "remark": "^10.0.0",
    "unist-builder-blueprint": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "^6.0.0",
    "nixt": "^0.5.0",
    "prettier": "^1.17.1",
    "xo": "^0.24.0"
  },
  "scripts": {
    "format": "prettier --write \"**/*.js\" && xo --fix",
    "test": "mocha"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "ignores": [
      "test/data"
    ]
  }
